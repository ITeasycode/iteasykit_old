<% content_for :entity_title do %>Editing <%= @cell.id %> - <%= @entity_type.name %>
<% end %>
<% content_for :pull_right do %>
  <%= link_to '<i class="zmdi zmdi-long-arrow-return zmdi-hc-fw"></i>'.html_safe, admin_entity_type_url(@entity_type) %>
<% end %>

<ul class="nav nav-tabs text-right">
  <li class="active">
    <a href="#"><%= t('editing') %> <%= @cell.id %></a>
  </li>

  <li>
    <a href="<%= new_admin_cell_path %>?entity_type=<%= @entity_type.id %>"><%= t('new') %> <%= @entity_type.name %></a>
  </li>

  <li>
    <a href="<%= admin_entity_type_url(@entity_type) %>" title="<%= t('list') %> <%= @entity_type.name %>"><i class="zmdi zmdi-playlist-plus"></i></a>
  </li>
  <li class="">
    <a href="<%= edit_admin_entity_type_url(@entity_type) %>" title="<%= t('edit_entity_type') %> <%= @entity_type.name %>"><i class="zmdi zmdi-settings"></i></a>
  </li>
  <li>
    <a href="<%= admin_entity_type_fields_url(@entity_type) %>" title="<%= t('fields_entity_type') %> <%= @entity_type.name %>"><i class="zmdi zmdi-border-color"></i></a>
  </li>

</ul>
<%= render 'form', cell: @cell %>