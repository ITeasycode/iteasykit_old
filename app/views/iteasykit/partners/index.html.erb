<p id="notice"><%= notice %></p>

<h1>Partners</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Site</th>
      <th>Phone</th>
      <th>Address</th>
      <th>Coordinates</th>
      <th>City ukr</th>
      <th>City mold</th>
      <th>Active</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @partners.each do |partner| %>
      <tr>
        <td><%= partner.name %></td>
        <td><%= partner.site %></td>
        <td><%= partner.phone %></td>
        <td><%= partner.address %></td>
        <td><%= partner.coordinates %></td>
        <td><%= partner.city_ukr %></td>
        <td><%= partner.city_mold %></td>
        <td><%= partner.active %></td>
        <td><%= link_to 'Show', partner %></td>
        <td><%= link_to 'Edit', edit_partner_path(partner) %></td>
        <td><%= link_to 'Destroy', partner, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Partner', new_partner_path %>
